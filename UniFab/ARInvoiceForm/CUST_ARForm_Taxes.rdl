    <DataSet Name="InvcTax">
      <Query>
        <DataSourceName>dsARForm</DataSourceName>
        <CommandText>="SELECT T1.CurrencyCode_DecimalsGeneral,T1.Calc_TaxMethod, T2.Company,T2.InvoiceNum,T2.InvoiceLine,T2.Calc_TaxDesc,T2.Calc_TaxableAmtTotal,T2.Calc_TaxAmtTotal,T2.[Percent],T2.Calc_LegalText,T3.Calc_TaxDesc as Label_Calc_TaxDesc,T3.RptLiteralsLHdng as Label_InvoiceNum,T3.[Percent] as Label_Percent,T3.RptLiteralsLAmt,T3.RptLiteralsLLegalText,T3.RptLiteralsLLine,T3.RptLiteralsLTotal,T3.TaxableAmt as Label_TaxableAmt
          FROM InvcHead_" + Parameters!TableGuid.Value + " T1
          LEFT OUTER JOIN InvcTax_" + Parameters!TableGuid.Value + " T2
          ON T1.Company = T2.Company AND T1.InvoiceNum = T2.InvoiceNum
          LEFT OUTER JOIN RptLabels_" + Parameters!TableGuid.Value + " T3
          ON T1.RptLanguageID = T3.RptLanguageID"</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
